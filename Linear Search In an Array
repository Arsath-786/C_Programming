#include <stdio.h>
int main()
{
    int arr[100],n,i,key;
    printf("enter the number of elements:");
    scanf("%d",&n);
    printf("enter the elements:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("enter the elements to search:\n");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    {
        if(arr[i]==key)
        {
            printf("%d key is found at index %d\n",key,i);
        }
    }
    printf("%d the key is not found\n",key);
    return 0;
}
