#include<stdio.h>
#include<math.h>
int main()
{
    int num,result;
    printf("Enter The Number:");
    scanf("%d",&num);
    if(num<0)
    {
        printf("square root of a negative number is not defined\n");
    }
    else
    {
        result=sqrt(num);
        printf("square root of %d is %d",num,result);
    }
    return 0;
}
