/*square root is the number multplied by itself
example:square root of 9 is 3 == 3*3=9
        square root of 25 is 5 == 5*5=25*/
#include<stdio.h>
#include<math.h>
int main()
{
    int num,result;
    printf("Enter The Number:");
    scanf("%d",&num);
    if(num<0)
    {
        printf("square root of a negative number is not defined\n");
    }
    else
    {
        result=sqrt(num);
        printf("square root of %d is %d",num,result);
    }
    return 0;
}
