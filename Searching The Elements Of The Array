#include<stdio.h>
int main()
{
    int key,arr[100],i,n;
    printf("Enter the Size Of the Array:\n");
    scanf("%d",&n);
    printf("Enter The Elements Of The Array:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("Enter the Element To Be Search:\n");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    {
        if (arr[i]==key)
        {
            printf("The Element is found at position %d\n",i+1);
            return 0;
        }
    }
    if(key!=0)
    {
        printf("The element is not Found\n");
    }
}
