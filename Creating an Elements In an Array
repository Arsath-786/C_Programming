#include<stdio.h>
int main()
{
    int i,n,arr[90];
    printf("Enter The Size of an Array:");
    scanf("%d",&n);
    printf("Enter the elements of an Array:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("The Elements In th Array Are:\n");
    for(i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }
    return 0;
}
