#include <stdio.h>
int main()
{
    int arr[100],n,i,sum=0;
    printf("enter the number of element:");
    scanf("%d",&n);
    printf("enter the elements:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        sum=sum+arr[i];
    }
    printf("sum of all elements = %d\n",sum);
    return 0;
}
